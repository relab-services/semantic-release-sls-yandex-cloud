{
    "name": "@relab/semantic-release-sls-yandex-cloud",
    "version": "0.0.0-development",
    "description": "semantic-release plugin to deploy serverless app to yandex cloud",
    "main": "lib/index.js",
    "files": [
        "README.md",
        "LICENSE",
        "lib"
    ],
    "scripts": {
        "build": "tsc",
        "prepublish": "npm run build",
        "semantic-release": "semantic-release",
        "lint": "eslint . --ext ts",
        "format": "prettier --write \"*.{js,json,md}\""
    },
    "keywords": [
        "semantic-release",
        "yandex-cloud"
    ],
    "author": "Sergey Zwezdin <sergey@zwezdin.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/relab-services/semantic-release-sls-yandex-cloud.git"
    },
    "bugs": {
        "url": "https://github.com/relab-services/semantic-release-sls-yandex-cloud/issues"
    },
    "homepage": "https://github.com/relab-services/semantic-release-sls-yandex-cloud#readme",
    "publishConfig": {
        "access": "public"
    },
    "peerDependencies": {
        "semantic-release": ">=20.1.0"
    },
    "devDependencies": {
        "@relab/eslint-config": "^2.2.3",
        "@relab/prettier-config": "^1.1.3",
        "@relab/semantic-release-commits-config": "^1.0.8",
        "@semantic-release/github": "^9.2.5",
        "@semantic-release/npm": "^11.0.2",
        "eslint": "^8.56.0",
        "semantic-release": "^22.0.10",
        "semantic-release-telegram-bot": "^2.0.1",
        "ts-node": "^10.9.2",
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@semantic-release/error": "^2.2.0",
        "execa": "^5.1.1"
    },
    "prettier": "@relab/prettier-config",
    "eslintConfig": {
        "extends": "@relab/eslint-config/node",
        "ignorePatterns": [
            "lib"
        ]
    }
